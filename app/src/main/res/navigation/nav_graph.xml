<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.spaapp.MainActivity"
        android:label="activity_main"
        app:action="Click"
        tools:layout="@layout/activity_main"/>
    <activity
        android:id="@+id/doggoDescription"
        android:name="com.example.spaapp.DoggoDescription"
        android:label="activity_doggo_description"
        tools:layout="@layout/activity_doggo_description" />
    <fragment
        android:id="@+id/doggoFragment2"
        android:name="com.example.spaapp.spa.DoggoFragment"
        android:label="DoggoFragment" >
        <action
            android:id="@+id/action_doggoFragment2_to_doggoDescription2"
            app:destination="@id/doggoDescription" />
    </fragment>
</navigation>